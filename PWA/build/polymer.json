{
  "entrypoint": "index.html",
  "shell": "src/Pwa-app/Pwa-app.js",
  "fragments": [],
  "sources": [
    "assets/*.*",
    "src/**/*.*",
    "index.html",
    "src/Pwa-app/Pwa-app.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json",
    "node_modules/@webcomponentjs/*.*"
  ],
  "builds": [
    {
      "name": "es6prod",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": true,
        "compile": "es2015",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-bundled",
      "swPrecacheConfig": "sw-precache-config.js"
    }
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}